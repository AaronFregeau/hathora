{{#each types}}
{{#if (eq type "enum")}}
export enum {{@key}} {
  {{#each options}}
  {{label}},
  {{/each}}
}
{{else if (eq type "object")}}
export type {{@key}} = {
  {{#each properties}}
  {{@key}}: {{> renderArg}};
  {{/each}}
};
{{else}}
export type {{@key}} = {{> renderArg}};
{{/if}}
{{/each}}
{{#each methods}}
export interface {{makeRequestName @key}} {
  {{#each properties}}
  {{@key}}: {{> renderArg}};
  {{/each}}
}
{{/each}}
{{#*inline "renderArg"}}
{{#if alias}}
{{typeString}}
{{~else if (eq type "optional")}}
{{> renderArg item}} | undefined
{{~else if (eq type "array")}}
{{> renderArg items}}[]
{{~else if (eq type "number")}}
number
{{~else if (eq type "string")}}
string
{{~else if (eq type "boolean")}}
boolean
{{~else if (eq type "plugin")}}
{{> renderArg item}}
{{~/if}}
{{~/inline}}